
    
def is_abnormal_blood_pressure(patient_info)
    return patient_info['systolic_bp'] > 140 or patient_data['diastolic_bp']

def is_irregular_respiratory_rate(patient_info)
    return patient_info['respiratory_rate'] < 12 or patient_info['respiratory_rate'] 

def create_alert(patient_info):
    if is_high_temperature(patient_info)
        send_notification("High Temperature Alert", "Patient ID: " + patient_info['id'] + " has a high temperature.")
     if is_high_heart_rate(patient_info)
        send_notification("High Heart Rate Alert", "Patient ID: " + patient_info['id'] + " has a high heart rate.")
     if is_abnormal_blood_pressure(patient_info)
         send_notification("Abnormal Blood Pressure Alert", "Patient ID: " + patient_info['id'] + " has abnormal blood pressure.")
    if is_irregular_respiratory_rate(patient_info):
    send_notification("Irregular Respiratory Rate Alert", "Patient ID: " + patient_data['id'] + " has an irregular respiratory rate.")
